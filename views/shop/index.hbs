{{# each products}}
<div class="row">
  {{# each this}}
    <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{this.imagePath}}" alt="..."
      class="img-responsive">
      <div class="caption">
        <h4>{{this.title}}</h4>
        <p class="description">{{this.description}}</p>
         <div class="clearfix">
             <div class="price pull-left">BWP: {{this.price}}</div>
          <a href="#" class="btn  pull-right btn-success" role="button">Add To Cart</a></p>
         </div>
      </div>
    </div>
   </div>
  {{/ each}}
{{/each}}